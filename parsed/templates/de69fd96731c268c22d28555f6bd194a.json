{
  "components/common/header": "<header class=\"header\" role=\"banner\">\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n        <span class=\"mobileMenu-toggleIcon\"></span>\n    </a>\n\n    <div class=\"header-logo--wrap\">\n        <div class=\"header-logo {{#if theme_settings.logo_size '===' 'original'}}logo-full-size{{/if}}\">\n\n            <div class=\"logo-and-toggle\">\n                <div class=\"desktopMenu-toggle\">\n                    <a href=\"javascript:void(0);\" class=\"mobileMenu-toggle\">\n                        <span class=\"mobileMenu-toggleIcon\"></span>\n                    </a>\n                </div>\n\n                {{#if page_type '===' 'default'}}\n                <h1 class=\"logo-wrap\">{{> components/common/store-logo}}</h1>\n                {{else}}\n                <div class=\"logo-wrap\">{{> components/common/store-logo}}</div>\n                {{/if}}\n            </div>\n            <!-- AH EDIT -->\n            <div class=\"navPages-container temp-menu\" style=\"flex: auto; z-index: 0;\" id=\"menu-two\" data-menu>\n                {{> components/common/navigation-menu}}\n            </div>\n\n            <div class=\"navUser-wrap\">\n                {{> components/common/navigation}}\n            </div>\n        </div>\n        <div class=\"navUser-wrap hidden\" style=\"display: none;\">\n            {{> components/common/navigation}}\n        </div>\n    </div>\n    <div class=\"navPages-container temp-menu hidden\" id=\"menu\" data-menu>\n        {{> components/common/navigation-menu}}\n    </div>\n    <div class=\"quickSearchWrap hidden\" style=\"display:none;\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\"\n    data-prevent-quick-search-close>\n    {{> components/common/quick-search}}\n    </div>\n</header>\n<!-- {{#unless page_type '===' 'default'}}\n\n{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#each (limit banners.top_metadata 1)}}\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\"\n        data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n        <div data-event-type=\"promotion-click\">\n            {{{this.content}}}\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{/if}}\n\n{{/unless}} -->\n\n{{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}}",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image}}\" alt=\"SISU Mouthguards\" title=\"{{settings.store_logo.title}}\">\n        </div>\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/quick-search": "<div>\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <button type=\"submit\" class=\"form-button\">\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-search\" /></svg>\n                    </i>\n                </button>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n",
  "components/common/navigation": "<nav class=\"navUser\" role=\"menu\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        <li class=\"navUser-item navUser-item--account\">\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg><use xlink:href=\"#icon-user\" /></svg>\n            </i>\n            {{#if customer}}\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a> or\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            {{else}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span>{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n                {{/if}}\n            {{/if}}\n        </li>\n        {{!--\n        {{> components/common/currency-selector}}\n        <li class=\"navUser-item navUser-item--mobile-search\">\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg><use xlink:href=\"#icon-search\" /></svg>\n            </i>\n        </li>\n        --}}\n        <li class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\"\n                aria-label=\"Cart with {{cart.quantity}} items\"\n            >\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg><use xlink:href=\"#icon-cart\" /></svg>\n                </i>\n                (<span class=\"{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>)\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n</nav>\n",
  "components/common/navigation-menu": "<span class=\"mobile-closeNav\">\n    <i class=\"icon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-close-large\" /></svg>\n    </i>\n    close\n</span>\n<nav class=\"navPages\" role=\"navigation\">\n    <ul class=\"navPages-list navPages-list--user 5765697a656e20596f756e67\">\n        {{#if customer}}\n            <li class=\"navPages-item\">\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg><use xlink:href=\"#icon-user\" /></svg>\n                </i>\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.orders.all}}\"\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\n                            >\n                                {{lang 'account.nav.orders'}}\n                            </a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.returns}}\"\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\n                                >\n                                    {{lang 'account.nav.returns'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.inbox}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\n                            >\n                                {{lang 'account.mobile_nav.messages'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.addresses}}\"\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\n                            >\n                                {{lang 'account.nav.addresses'}}\n                            </a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.payment_methods.all}}\"\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\n                                >\n                                    {{lang 'account.nav.payment_methods'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.wishlists.all}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\n                            >\n                                {{lang 'account.mobile_nav.wishlists'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.recent_items}}\"\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\n                            >\n                                {{lang 'account.nav.recently_viewed'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.details}}\"\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\n                            >\n                                {{lang 'account.nav.settings'}}\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg><use xlink:href=\"#icon-user\" /></svg>\n                </i>\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                {{#if settings.account_creation_enabled}}\n                    {{lang 'common.or'}}\n                    <a class=\"navPages-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/if}}\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item navPages-item--currency\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n    <ul class=\"navPages-list navPages-mega navPages-mainNav\">\n<!-- AH EDIT -->\n        {{#if theme_settings.show_content_navigation}}\n        {{#each (limit pages 3)}}\n        <li class=\"navPages-item\">\n            {{> components/common/navigation-list}}\n        </li>\n        {{/each}}\n        {{/if}}\n    </ul>\n    <!-- <ul class=\"navPages-list navPages-list--user\">\n        {{#and settings.phone_number theme_settings.header-phone}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navUser-action--phone\" href=\"tel:{{settings.phone_number}}\"><i class=\"fa fa-phone\" aria-hidden=\"true\"></i>&nbsp;{{settings.phone_number}}</a>\n            </li>\n        {{/and}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navPages-item\">\n                {{> components/common/social-links}}\n            </li>\n        {{/if}}\n    </ul> -->\n</nav>\n",
  "components/common/currency-selector": "<!-- {{#if currency_selector.currencies.length '>' 1}}\n    <li class=\"navUser-item navUser-item-currency\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-downselector-small\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n{{/if}} -->\n<!-- <script>\n    function chatNow() {\n        let chatApp = document.getElementById('tidio-chat');\n        if(chatApp.style.display !== 'block') {\n            chatApp.style.display = 'block'; tidioChatApi.open();\n        } else {\n            tidioChatApi.close(); chatApp.style.display = 'none';\n        }\n    }\n</script> -->\n\n<li class=\"navUser-item navUser-item--chat\">\n<a style=\"display: inline-block;\" class=\"navUser-action navUser-action--currencySelector\" href=\"#\" onclick=\"chatNow();\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">\n
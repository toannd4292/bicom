{"components/common/requireness-msg":"<small {{#if required}}class=\"is-required\"{{/if}}>\n    {{#if required}}{{lang 'common.required'}}{{else}}{{lang 'common.optional'}}{{/if}}\n</small>\n","components/products/options/set-rectangle":"<div class=\"form-field xcrs\" data-product-attribute=\"set-rectangle\" role=\"radiogroup\" aria-labelledby=\"rectangle-group-label\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall grtdvrs\" id=\"rectangle-group-label\">\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    {{#unless required}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_rectangle__{{../id}}_none\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"\"\r\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\r\n        >\r\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_none\">\r\n            <span class=\"form-option-variant\">{{lang 'products.none'}}</span>\r\n        </label>\r\n    {{/unless}}\r\n\r\n    {{#each this.values}}\r\n    <div class=\"form-option-wrapper\">    \r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_rectangle__{{../id}}_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}\r\n        >\r\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n"}

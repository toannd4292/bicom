{
  "layout/body-only": "<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\r\n        <script>\r\n            // Change document class from no-js to js so we can detect this in css\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n\r\n        {{#if theme_settings.csspreload '===' ''}}\r\n            {{#if theme_settings.use_wow}}\r\n                <link href=\"{{cdn 'assets/vendor/animate/animate.min.css'}}\" rel=\"stylesheet\">\r\n            {{/if}}\r\n            {{ getFontsCollection }}\r\n            {{{stylesheet '/assets/css/theme.css'}}}\r\n        {{else}}\r\n            <script>document.write('{{{ getFontsCollection }}}'.replace(/:[i,0-9]{3,}/g, '').replace('rel=\"stylesheet\"', 'rel=\"preload\" as=\"style\" onload=\"this.onload=null;this.rel=\\'stylesheet\\'\"'));</script>\r\n            <noscript>{{ getFontsCollection }}</noscript>\r\n            \r\n            {{#if theme_settings.use_wow}}\r\n                {{{stylesheet '/assets/vendor/animate/animate.min.css' rel=\"preload\" as=\"style\" onload=\"this.onload=null;this.rel='stylesheet'\"}}}\r\n                <noscript>{{{stylesheet '/assets/vendor/animate/animate.min.css'}}}</noscript>\r\n            {{/if}}\r\n\r\n            <script src=\"{{cdn 'assets/vendor/loadcss/cssrelpreload.min.js'}}\" async></script>\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.csspreload '===' 'performance'}}\r\n            <style>@media (max-width: 800px) { .pace, .nanobar { display: none } }</style>\r\n            {{{stylesheet '/assets/css/theme.css'}}}\r\n            <script>window.stencilStyleLoaded = true;</script>\r\n            <script>window.lazySizesConfig = { loadHidden: false, expand: 150, expFactor: 3 };</script>\r\n\r\n        {{else if theme_settings.csspreload '===' 'speedtest'}}\r\n            <meta class=\"foundation-data-attribute-namespace\">\r\n            <style id=\"themeCSSPreload\">\r\n                meta.foundation-data-attribute-namespace { font-family: false }\r\n                body { visibility: hidden !important }\r\n                .emthemesModez-header-userSection > .container { transition: none !important }\r\n            </style>\r\n            <style>@media (max-width: 800px) { .pace, .nanobar { display: none } }</style>\r\n            {{{stylesheet '/assets/css/theme.css' rel=\"preload\" as=\"style\" onload=\"this.onload=null;this.rel='stylesheet';var el=document.getElementById('themeCSSPreload');el.parentNode.removeChild(el);window.stencilStyleLoaded=true;window.stencilAutoload&&window.stencilAutoload()\"}}}\r\n            <noscript>{{{stylesheet '/assets/css/theme.css'}}}</noscript>\r\n            <script>window.lazySizesConfig = { loadHidden: false, expand: 0 };</script>\r\n        {{/if}}\r\n\r\n        {{#block \"head-after\"}} {{/block}}\r\n\r\n        {{{head.scripts}}}\r\n        {{{head.rsslinks}}}\r\n\r\n        {{inject 'themeSettings' theme_settings}}\r\n        {{inject 'genericError' (lang 'common.generic_error')}}\r\n        {{inject 'maintenanceMode' settings.maintenance}}\r\n        {{inject 'urls' urls}}\r\n        {{inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{inject 'pageType' page_type}}\r\n        {{inject 'cartId' cart_id}}\r\n        {{inject 'template' template}}\r\n        {{inject 'pageTitle' page.title}}\r\n        {{inject 'product' product}}\r\n        {{{snippet 'htmlhead'}}}\r\n    </head>\r\n    <body class=\"{{#if theme_settings.navigation_hover}}navigation-hover{{/if}} page-type-{{page_type}}\r\n        {{#if theme_settings.product_outstock_badge}}theme-show-outstock-badge{{/if}}\r\n        {{#if theme_settings.header_sticky}}theme-header-sticky{{/if}}\r\n        \">    \r\n        {{#if theme_settings.fb_page '!==' ''}}\r\n            <!-- Facebook SDK -->\r\n            <div id=\"fb-root\"></div>\r\n            <script>(function(d, s, id) {\r\n            var js, fjs = d.getElementsByTagName(s)[0];\r\n            if (d.getElementById(id)) return;\r\n            js = d.createElement(s); js.id = id;\r\n            js.src = \"//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.10&appId=378187145615477\";\r\n            fjs.parentNode.insertBefore(js, fjs);\r\n            }(document, 'script', 'facebook-jssdk'));</script>\r\n        {{/if}}\r\n        \r\n        {{{snippet 'header'}}}\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n\r\n        {{> components/common/body }}\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        {{#if theme_settings.csspreload}}\r\n            <script>window.stencilBootstrapArgs = { page_type: \"{{page_type}}\", jsContext: {{jsContext}}, autoload: true };</script>\r\n            <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\" async></script>\r\n        {{else}}\r\n            <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n            <script>\r\n                // Exported in app.js\r\n                window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n            </script>\r\n        {{/if}}\r\n\r\n        {{{footer.scripts}}}\r\n        {{{snippet 'footer'}}}\r\n        {{#block \"footer-scripts\"}} {{/block}}\r\n    </body>\r\n</html>\r\n",
  "components/common/body": "<div class=\"body{{#if template_file '===' 'pages/home'}} home{{/if}}\" id='main-content' role='main' data-currency-code=\"{{currency_selector.active_currency_code}}\">\n    {{> components/sisu/announcement-bar}}\n    {{#block \"hero\"}} {{/block}}\n    {{# block \"page-before\"}} {{/block}}\n    {{#block \"page\"}} {{/block}}\n    {{# block \"page-after\"}} {{/block}}\n    {{#block \"hero-bottom\"}} {{/block}}\n    {{> components/common/modal/modal}}\n    {{> components/common/alert/alert-modal}}\n</div>\n",
  "components/sisu/announcement-bar": "{{#unless page_type '===' 'cart'}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n  <div class=\"owl-carousel owl-banner owl-theme owl-loaded\">\r\n      <div class=\"owl-stage-outer\">\r\n          <div class=\"owl-stage\">\r\n              <div class=\"owl-item\">\r\n                  <div data-event-type=\"promotion-click\">\r\n                      <p>All Bundles 20% Off! <a class=\"button button--small button--banner\" href=\"/categories/bundles/\" onclick='ga(\"send\", \"event\", \"bundle-sale-2022\", \"ClickedButton\",\"AnnouncementBar\");'>click here</a></p>\r\n                  </div>\r\n              </div>\r\n              <div class=\"owl-item\">\r\n                <div data-event-type=\"promotion-click\">\r\n                    <p>New Season Saver Bundle <a class=\"button button--small button--banner\" href=\"/season-saver-bundle/\" onclick='ga(\"send\", \"event\", \"season-saver-bundle\", \"ClickedButton\",\"AnnouncementBar\");'>shop now</a></p>\r\n                </div>\r\n            </div>\r\n            <div class=\"owl-item\">\r\n                <div data-event-type=\"promotion-click\">\r\n                    <p><strong>FREE RAIN PONCHO</strong> with all bundles!</p>\r\n                </div>\r\n              </div>\r\n              <div class=\"owl-item\">\r\n                <div data-event-type=\"promotion-click\">\r\n                    <p><strong>FREE SHIPPING</strong> on all orders! (US only)</p>\r\n                </div>\r\n              </div>\r\n              <div class=\"owl-item\">\r\n                  <div data-event-type=\"promotion-click\">\r\n                      <p>(see'-soo) <em>noun</em> - bravery, tenacity, <strong>grit</strong></p>\r\n                  </div>\r\n              </div>\r\n          </div>\r\n      </div>\r\n  </div>\r\n</div>\r\n{{/unless}}",
  "components/common/modal/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    {{> components/common/modal/modal-close-btn}}\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n",
  "components/common/alert/alert-modal": "<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\n    <div class=\"swal2-icon swal2-error swal2-icon-show\"><span class=\"swal2-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\n\n    <div class=\"modal-content\"></div>\n\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\n</div>\n",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\n        type=\"button\"\n        title=\"{{lang 'common.close'}}\"\n        onclick=\"window.location.reload()\"\n        {{additional_attr}}\n>\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n    <span aria-hidden=\"true\">&#215;</span>\n</button>\n"
}

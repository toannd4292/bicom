     </i>\r\n            {{#if customer}}\r\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a> or\r\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n            {{else}}\r\n                <a class=\"navUser-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                    {{lang 'common.login'}}\r\n                </a>\r\n                {{#if settings.account_creation_enabled}}\r\n                    <span>{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                {{/if}}\r\n            {{/if}}\r\n        </li>\r\n        {{!--\r\n        {{> components/common/currency-selector}}\r\n        <li class=\"navUser-item navUser-item--mobile-search\">\r\n            <i class=\"icon\" aria-hidden=\"true\">\r\n                <svg><use xlink:href=\"#icon-search\" /></svg>\r\n            </i>\r\n        </li>\r\n        --}}\r\n        <li class=\"navUser-item navUser-item--cart\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\"\r\n                aria-label=\"Cart with {{cart.quantity}} items\"\r\n            >\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-cart\" /></svg>\r\n                </i>\r\n                (<span class=\"{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>)\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item navPages-item--currency\">\r\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\r\n                     {{#if currency_selector.active_currency_code '===' 'GBP'}} £ {{/if}} {{#if currency_selector.active_currency_code '===' 'EUR'}} € {{/if}} {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                {{#if is_active}}\r\n                                    {{#if name '===' 'British Pound'}} £ {{/if}} {{#if name '===' 'Euro'}} € {{/if}} <strong class=\"name-cure\">{{name}}</strong>\r\n                                {{else}}\r\n                                    {{#if name '===' 'British Pound'}} £ {{/if}} {{#if name '===' 'Euro'}} € {{/if}} {{name}}\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}        \r\n    </ul>\r\n</nav>\r\n \r\n"}
